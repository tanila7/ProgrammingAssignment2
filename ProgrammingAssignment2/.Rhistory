makeCacheMatrix <- function(x = matrix()) {
s <- NULL
set <- function(y){
x <<- y
s <<- NULL
}
get <- function() x
setsolve <- function(solve) s <<- solve
getsolve <- function() s
list(set = set,
get = get,
setsolve = setsolve,
getsolve = getsolve)
}
makeCacheMatrix(x)
M<-makeCacheMatrix(x)
M
cacheSolve(M)
cacheSolve <- function(x, ...) {
s <- x$getsolve()
if(!is.null(s)) {
message("getting cached data")
return(s)
}
data <- x$get()
s <- solve(data, ...)
x$setsolve(s)
s
}
cacheSolve(M)
M$set(1:4,2,2)
M$set(1:4)
M
M$set(matrix(1:4,2,2))
M
cacheSolve(M)
M%**%cacheSolve(M)
M%*%cacheSolve(M)
matrix(1:4,2,2)%*%cacheSolve(M)
